<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="center"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tvFirstNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/tvSecondNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/tvSecondNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:minHeight="48dp"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/tvThirdNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvFirstNumber"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/tvThirdNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/tvFourthNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvSecondNumber"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvFourthNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/tvFifthNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvThirdNumber"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvFifthNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/tvSixthNumber"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvFourthNumber"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/tvSixthNumber"
        android:layout_width="40dp"
        android:layout_height="48dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="15dp"
        android:ems="10"
        android:importantForAutofill="no"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tvFifthNumber"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="LabelFor,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btnGenerator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:text="Button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvThirdNumber" />
</androidx.constraintlayout.widget.ConstraintLayout>